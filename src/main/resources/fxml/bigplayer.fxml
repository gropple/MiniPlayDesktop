<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="mainWindow" maxHeight="90.0" prefHeight="90.0" prefWidth="111.0" styleClass="root" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.BigPlayerController">
   <children>
      <HBox prefHeight="0.0">
         <children>
            <ImageView fx:id="art" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
            <VBox prefHeight="90.0" prefWidth="250.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="title" prefHeight="20.0" prefWidth="545.0" text="Not connected to Google Music">
                     <font>
                        <Font name="Roboto" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="artist" prefHeight="0.0">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <HBox id="controllerButtons" fx:id="buttonBar" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets left="10.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
